<#UniversityMapping>
 rml:logicalSource [ 
 rml:source "HandsOn/Group08/csv2/us-colleges-and-universities-modified.csv";
 rml:referenceFormulation ql:CSV
 ];
 rr:subjectMap [
    rr:template
      "http://www.semanticweb.org/upm/opendata/group08/ontologies/ns#{IPEDSID}";
    rr:termType rr:IRI;
    rr:class ns:University;
  ];
  rr:predicateObjectMap [ 
  rr:predicate foaf:name; 
  rr:objectMap [ 
  rml:reference "NAME"; 
 rr:termType rr:Literal;
 rr:datatype xsd:string;
 ] 
  ];
     rr:predicateObjectMap [ 
  rr:predicate rdf:type; 
  rr:objectMap [ 
   rr:constant ns:University;
 ] 
  ];
  rr:predicateObjectMap [ 
  rr:predicate schema:latitude; 
  rr:objectMap [ 
  rml:reference "LATITUDE"; 
 rr:termType rr:Literal;
 rr:datatype xsd:string;
 ] 
  ];
  rr:predicateObjectMap [ 
  rr:predicate schema:longitude;
  rr:objectMap [ 
  rml:reference "LONGITUDE";
 rr:termType rr:Literal;
 rr:datatype xsd:string
 ] 
  ];
  rr:predicateObjectMap [ 
  rr:predicate dbo:city; 
  rr:objectMap [ 
  rr:template "https://dbpedia.org/page/{CITY}";
] 
];
rr:predicateObjectMap [ 
  rr:predicate ns:Website;
  rr:objectMap [ 
  rml:reference "WEBSITE";
  rr:termType rr:Literal;
  rr:datatype xsd:string
] 
];
rr:predicateObjectMap [ 
  rr:predicate owl:sameAs;
  rr:objectMap [ 
  rr:template "https://dbpedia.org/page/{NAME}";
] 
];
rr:predicateObjectMap [ 
  rr:predicate dbo:country;
  rr:objectMap [ 
  rr:template "https://dbpedia.org/page/{COUNTRY}";
] 
];
rr:predicateObjectMap [
  rr:predicate dbo:county; 
  rr:objectMap [ 
  rr:template "https://dbpedia.org/page/{COUNTY}";
] 
];
rr:predicateObjectMap [
  rr:predicate ns:state; 
  rr:objectMap [ 
    rml:reference "WEBSITE";
    rr:termType rr:Literal;
    rr:datatype xsd:string;
] 
].
 