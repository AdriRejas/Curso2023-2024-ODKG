@base <http://www.semanticweb.org/upm/opendata/group08/ontologies/ns#> .
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ns: <http://www.semanticweb.org/upm/opendata/group08/ontologies/ns#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

<#UniversityRankingMapping>
 rml:logicalSource [ 
 rml:source "HandsOn/Group08/csv2/US-News-Rankings-Universities-Through-2023-updated2.csv";
 rml:referenceFormulation ql:CSV
 ];
 rr:subjectMap [
    rr:template
      "http://www.semanticweb.org/upm/opendata/group08/ontologies/ns#{IPEDSID}-{Year}";
    rr:termType rr:IRI;
  ];
    rr:predicateObjectMap [ 
  rr:predicate ns:yearPublished; 
  rr:objectMap [ 
  rml:reference "Year"; 
 rr:termType rr:Literal;
 rr:datatype xsd:number;
 ] 
  ];
    rr:predicateObjectMap [ 
  rr:predicate ns:Score; 
  rr:objectMap [ 
  rml:reference "Ranking"; 
 rr:termType rr:Literal;
 rr:datatype xsd:string;
 ] 
  ].